        System.out.printf("ans="+input);}
}
11)import java.util.Scanner;
public class main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        double input = scanner.nextDouble();
        input=((input - 32) * 5 / 9);
        System.out.printf("%.2f",input);}
}
12)import java.util.Scanner;
public class main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        double input = scanner.nextDouble();
        input=input* 0.08333 ;
        System.out.printf("%.2f",input);}
}
13)import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	Scanner obj=new Scanner(System.in);
	int limit=obj.nextInt();
	for(int i=0;i<limit;i++)
		System.out.println("fazzz");
	}
}
13)import java.util.Scanner;
public class main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int input = scanner.nextInt();
        if(input%4==0 && input%400==0)
           System.out.print("the year is leap year");
        else
           System.out.print("the yaer is not leap year");
    }
}
15)import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	Scanner obj=new Scanner(System.in);
	int limit=obj.nextInt();
	for(int i=0;i<limit;i++)
    if(i%2==0)
      System.out.print(i);
	}
}
16)import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    Scanner obj=new Scanner(System.in);
	    int input1=obj.nextInt();
	    int input2=obj.nextInt();
	    if(input1<25 && input2==1)
		  System.out.println("group 1");
		else if(input1<=25 && input2==2)
		  System.out.println("group 2");
		else if((input1>25 && input1<=45) && input2==1)
		  System.out.println("group 3");
		else if((input1>25 && input1<=45) && input2==2)
		  System.out.println("group 4");
		else if(input1<45 && (input2==1 || input2==2))
		  System.out.println("group 5");
		else 
		  System.out.println("invalid");
		   
	}
}
